/* components/searchbar/searchbar.wxss */
.searchBar_container{
  width:100%;
  height: 100rpx;
  display: flex;
}
.searchBar_body{
  width:100%;
  margin: 10rpx 20rpx;
  border-radius: 100rpx;
  background: #eee;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.search_input{
  width: 100%;
  font-size: 30rpx;
  margin:10rpx 40rpx;
  display: flex;
  justify-content: space-around;
}
.clear{
  width: 50rpx;
  height: 50rpx;
  margin-right: 30rpx;
}