.room_list_container{
  width:100%;
}
.rooms_group{
  width: 100%;
}
.room_parent{
  height: 110rpx;
  border-bottom: 2rpx solid #eee;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.room_parent_header{
  display: flex;
  align-items: center;
  padding-left:40rpx; 
}
.floor_name{
  font-size: 40rpx;
  padding-left: 40rpx;
  /* vertical-align:top; */
}
.toggle_floor{
  float: right;
  width: 40rpx;
  height: 40rpx;
  padding-right:40rpx; 
}
.hidden{
  display: none
}
.room_child_list{
  width:100%;
}
.room_child{
  padding:10rpx 40rpx;
  border-bottom: 2rpx solid #e8e8e8;
  background:#f5f5f5;
}
.room_name{
  font-size: 35rpx;
  display: inline-block;
}
.order{
  width:200rpx;
  height: 30rpx;
  float: right;
  /* font-size: 1rpx; */
  text-align: right;
}
.room_detail{
  font-size: 25rpx;
  color:#595959;
}
.order_details{
  margin-top: 10rpx;
  font-size: 28rpx;
  background: #d9d9d9;
  color: #8c8c8c;
  padding: 6rpx 0;
}
.order_details_text{
  padding-left: 20rpx
}