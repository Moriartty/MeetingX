.orderList_group{
  width: 100%;
}
.orderList_parent{
  width: 100%;
  border-bottom: 2rpx solid #eee;
}
.orderList_footer{
  text-align: center;
  font-size: 30rpx;
  color:#bfbfbf;
  margin-top: 20rpx;
  margin-bottom: 40rpx;
}
.item_header{
  text-align: left;
  color:#8c8c8c;
  font-size: 30rpx;
  border-bottom:2rpx solid #eee;
  padding: 10rpx 30rpx;
  display: flex;
  align-items: center;
}
.item_contents{
  width: 100%;
  margin-left:30rpx;
}
.item_content{
  height: 130rpx;
  border-bottom:2rpx solid #eee;
  display: flex;
  align-items: center;
}
.item_icon{
  background: #595959;
  height: 100rpx;
  width: 100rpx;
  color:white;
  border-radius: 10rpx;
  text-align: center;
  display: inline-block 
}
.item_body{
  width: 70%;
  display:inline-block;
  padding-left: 40rpx;
  vertical-align: top;

}
.meeting_title{
  color:#000000;
  font-size: 38rpx;
  display: block
}
.meeting_time,.meeting_room{
  color:#bfbfbf;
  font-size: 30rpx;
  margin-right: 40rpx;
}