.navbar {
    position: fixed;
    top:240rpx;
    display: flex;
    border-bottom: 2rpx solid #eee;
    width:100%;
    height:80rpx;
    background-color: white;
    color: black;
    z-index:10
}
.navbar-item {
    flex: 1.0;
    text-align: center;
    font-size: 30rpx;
    color: #999;
    margin-bottom: -1px;
}
.navbar-text {
    display: block;
    width: 200rpx;
    padding: 20rpx;
    margin: auto;
}
.navbar-text.active {
    border-bottom: 6rpx solid #000;
    color: #000;
    font-weight: bold;
}
.hidden{
  display:none;
}

.topMenu{
  background: #595959;
  width:100%;
  height: 240rpx;
  position:fixed;
  top:0;
  z-index: 10;
  display: flex;
  text-align: center;
  align-items: center;
  justify-content: space-between
}
.add_meeting,.open_draft{
  flex: 1.0;
  width: 120rpx;
  height: 220rpx;
  border-radius: 100rpx;
  text-align: center;    
  font-size: 30rpx;
  color: #ffffff;
  display: flex;
  flex-direction: column; 
  align-items: center;
  justify-content: space-around;
}
.add_meeting_button,.open_draft_button{
    width: 70rpx;
    height: 70rpx;
    border-radius: 50%;
    background-color: #cd950c;
    padding: 25rpx;
}
.nodata{
  width:100%;
  text-align: center;
  height: 800rpx;
  font-size: 35rpx;
  color:#bfbfbf;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}